body.outside {
	margin: 0;
	padding: 0;
	scrollbar-face-color: #E6E4E4;
	scrollbar-shadow-color: #FFFFFF;
	scrollbar-highlight-color: #FFFFFF;
	scrollbar-3dlight-color: #E6E4E4;
	scrollbar-darkshadow-color: #E6E4E4;
	scrollbar-track-color: #F0F0F0;
	scrollbar-arrow-color: #FFFFFF;
	font-family: "Î¢ÈíÑÅºÚ", Tahoma, Verdana, Arial, "ËÎÌå";
	font-size: 12pt;
}

/*** monitor table begin ***/
table.monitor {
	margin: 0;
	height: 100%;
	width: 100%;
	border: 1px solid #D1DBE7;
    border-collapse: collapse;
    font-family: "Î¢ÈíÑÅºÚ", Tahoma, Verdana, Arial, "ËÎÌå";
	font-size: 12pt;
	table-layout:fixed;
}

.monitor th {
	font-weight: normal;
	TEXT-ALIGN: center;
    BORDER: 1px solid #D1DBE7;
    COLOR: #000000;
    BACKGROUND-COLOR: #E7F1F3;
    HEIGHT: 25px;
}

.monitor th.title {
    HEIGHT: 25px;
	padding-left: 10px;
	font-weight: bold;
	TEXT-ALIGN: left;
	BACKGROUND: #E7F1F3 url(../images/ecTable/table_page_bg.gif) repeat-x left 50%;
}

.monitor td {
    text-align: center;
    border: 1px solid #D1DBE7;
	height: 25px;
}
/*** monitor table end ***/

div.chart {
	width: 100%;
	height: 100%;
	overflow: auto;
	margin: 0px;
	padding: 5px 3px;
}
